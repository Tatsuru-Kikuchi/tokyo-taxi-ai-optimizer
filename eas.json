{
  "cli": {
    "version": ">= 5.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "default"
      },
      "android": {
        "resourceClass": "default"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "default",
        "simulator": true
      },
      "android": {
        "resourceClass": "default",
        "buildType": "apk"
      }
    },
    "production": {
      "ios": {
        "resourceClass": "default",
        "buildConfiguration": "Release"
      },
      "android": {
        "resourceClass": "default",
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "tatsuru.kikuchi@icloud.com",
        "appleTeamId": "B67J43NU85"
      },
      "android": {
        "track": "production"
      }
    }
  }
}
